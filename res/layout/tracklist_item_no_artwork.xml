<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="76dip"
	android:padding="0dip">

	<ImageView android:id="@+id/track_image_plays"
		android:layout_width="16dip" android:layout_height="16dip"
		android:layout_gravity="center_vertical" />

	<TextView android:id="@+id/track_plays"
		android:layout_marginLeft="6dip" android:layout_width="64dip"
		android:layout_height="fill_parent" android:layout_toRightOf="@id/track_image_plays"
		android:layout_above="@+id/track_title" android:textAppearance="?android:attr/textAppearanceSmall"
		android:gravity="top" android:text="1201202" />

	<ImageView android:id="@+id/track_image_downloads"
		android:layout_width="16dip" android:layout_height="16dip"
		android:layout_marginLeft="6dip" android:layout_gravity="center_vertical"

		android:layout_toRightOf="@id/track_plays" />

	<TextView android:id="@+id/track_downloads"
		android:layout_marginLeft="0dip" android:layout_width="64dip"
		android:layout_height="fill_parent" android:layout_toRightOf="@id/track_image_downloads"
		android:layout_above="@id/track_title" android:textAppearance="?android:attr/textAppearanceSmall"
		android:gravity="top" android:text="1430009" />

	<TextView android:id="@+id/track_sharing"
		android:layout_marginLeft="6dip" android:layout_width="48dip"
		android:layout_height="fill_parent" android:layout_toRightOf="@id/track_downloads"


		android:textAppearance="?android:attr/textAppearanceSmall"
		android:gravity="top" android:text="Private" />

	<ImageView android:id="@+id/track_playing_now"
		android:layout_width="16dip" android:layout_height="16dip"
		android:layout_marginLeft="16dip" android:layout_gravity="center_vertical"

		android:layout_toRightOf="@id/track_sharing" />


	<TextView android:id="@id/track_title"
		android:layout_marginLeft="6dip" android:layout_width="fill_parent"
		android:layout_height="26dip" android:layout_below="@id/track_image_plays"
		android:scrollHorizontally="true" android:ellipsize="marquee"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="bottom" />

	<TextView android:id="@+id/track_username"
		android:layout_width="fill_parent" android:layout_height="26dip"

		android:layout_below="@id/track_title" android:layout_alignLeft="@id/track_title"
		android:layout_alignParentBottom="true" android:gravity="top" />

<ImageView android:id="@+id/throbber"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
		android:src="@drawable/ic_popup_sync_1"
		android:visibility="gone" />


</RelativeLayout>



